class Solution {
public:
    void merge(vector<int>& nums1, int m, vector<int>& nums2, int n) {
        int k = m+n-1;
        for (int j=n-1;j>=0;j--){
            for(int i=m-1;i>=0;i--){
                if(nums1[i] > nums2[j]){
                nums1[k--] = nums1[i];
                m--;
                }
            }
            nums1[k--] = nums2[j];
        }
    }
};
